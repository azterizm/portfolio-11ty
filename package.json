{
  "name": "portfolio",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "build:css": "NODE_ENV=production tailwindcss -i ./src/_includes/tailwind.css -o ./src/css/tailwind.css --minify",
    "build:site": "npx @11ty/eleventy",
    "build": "rm -rf _site && npm run build:css && npm run build:site",
    "start": "npx @11ty/eleventy --serve",
    "dev": "concurrently \"npm run start\" \"npm run css\" --kill-others",
    "watch": "npx @11ty/eleventy --watch",
    "css": "tailwindcss -i ./src/_includes/tailwind.css -o ./src/css/tailwind.css --watch"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "type": "module",
  "dependencies": {
    "@11ty/eleventy": "^3.0.0",
    "@11ty/eleventy-plugin-rss": "^2.0.4"
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.15",
    "concurrently": "^9.2.1",
    "tailwindcss": "^3.4.14"
  }
}
